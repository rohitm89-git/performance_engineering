              <stringProp name="script">//printing the thread name to logs
log.info(&quot;Thread name is: &quot;+ctx.getThread().getThreadName());
log.info(&quot;match count: &quot;+vars.get(&quot;departingCity_matchNr&quot;));

//saving the response body to a variable for processing
def responseBody = prev.getResponseDataAsString();

//save responseBody in a jmeter variable for using to extract destination cities in a pre-processor
vars.put(&quot;respBody&quot;, responseBody);

//process the responseBody
import java.util.regex.Pattern;
import java.util.regex.Matcher;

//loop through the responseBody and extract the depart city values and store in an array or arraylist - array code commented out
//def city_count = 7;
//def departCities = new String[city_count];
Pattern pattern;
Matcher matcher;

//if the list of cities is dynamic
import java.util.ArrayList;
ArrayList&lt;String&gt; departCities = new ArrayList&lt;String&gt;(10);
def i = 0;

while(true){
	pattern = Pattern.compile(&quot;name=\&quot;fromPort\&quot;.*&gt;((\\s*)(&lt;option.*&gt;(.*)&lt;/option&gt;)){&quot;+(i+1)+&quot;}&quot;, Pattern.CASE_INSENSITIVE);
	matcher = pattern.matcher(responseBody);

	if(matcher.find()){
//		departCities[i] = matcher.group(4);
		departCities.add(matcher.group(4));
		i++;
		//optional logging message
		log.info(&quot;group: &quot;+matcher.group(4)+&quot;\nstart_index: &quot;+matcher.start()+&quot;\nend_idx: &quot;+matcher.end());
	}else break;
}

